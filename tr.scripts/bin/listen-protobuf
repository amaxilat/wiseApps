#!/bin/bash
BASEDIR=`dirname $0`

if [ "$#" -lt 1 ]
then
	echo "Usage $0 PROPERTIES_FILE"
	echo "Example: $0 live.properties"
	exit 1
fi

resKey=$(cat ../resKey)
java -Dtestbed.secretreservationkeys=$resKey -jar $BASEDIR/../lib/tr.scripting-client-current.jar -p $1 -f $BASEDIR/../scripts/listen-protobuf.java -v
